{
  "name": "tlovendo-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "kill -9 $(lsof -t -i:3000) 2>/dev/null || true && cross-env PORT=3000 next dev",
    "build": "next build",
    "build:local": "node scripts/build-local-vehicles.js",
    "start": "next start",
    "lint": "next lint",
    "optimize-images": "node scripts/optimize-images.js",
    "watch-images": "node scripts/optimize-images.js --watch",
    "prebuild": "if [ \"$SKIP_PREBUILD\" = \"1\" ]; then echo 'Skipping prebuild'; else npm run build:local || true && npm run optimize-images || true; fi"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "@heroicons/react": "^2.2.0",
    "chokidar": "^3.5.3",
    "clsx": "^2.1.1",
    "fast-glob": "^3.3.2",
    "framer-motion": "^10.18.0",
    "fs-extra": "^11.2.0",
    "lucide-react": "^0.544.0",
    "next": "14.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.62.0",
    "sharp": "^0.33.0",
    "swiper": "^11.2.10",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.18",
    "cross-env": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.0"
  }
}
